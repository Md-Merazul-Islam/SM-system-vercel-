{
    "version": 2,
    "builds": [
      {
        "src": "school_management_system/wsgi.py",
        "use": "@vercel/python",
        "config": {
          "maxLambdaSize": "50mb",
          "runtime": "python"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "school_management_system/wsgi.py"
      }
     
    ]
  
  
  }
  